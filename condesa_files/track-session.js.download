function sp_gp(){var pt=document.querySelector('meta[property="og:type"]'),pu=document.querySelector('meta[property="og:url"');if(pt&&pt.content==='product'){return'product';}else if((pu&&pu.content.indexOf('/cart')!==-1)||window.location.href.includes('cart')){return"cart";}}function sp_ha(t){var e=document.querySelector('div[data-shopify="payment-button"]'),b=document.querySelector('div[data-shopify="dynamic-checkout-cart"]');if(t==='product'&&e){e.style.display='none'}else if(t==='cart'&&b){b.style.display='none'}}function sp_cl(t){var e=document.querySelector('button[name=add]');if(t==='product'&&e){e.addEventListener('click',function(){sp_uc('cart')},!1)}}function sp_gs(){var t='; '+document.cookie,e=t.split('; '+'ellipsis.sessionid'+'=');if(2==e.length)return e.pop().split(';').shift()}function sp_uc(t){var e='attributes[sp_last_visited]='+t+'&attributes[sp_sess_id]='+sp_gs();var sp_rq=new XMLHttpRequest();sp_rq.open('POST','/cart/update.js',!0),sp_rq.setRequestHeader('Content-Type','application/x-www-form-urlencoded; charset=UTF-8'),sp_rq.send(e)}var sp_cp=sp_gp();sp_uc(sp_cp);sp_cl(sp_cp);sp_ha(sp_cp);